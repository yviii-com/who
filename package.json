{
    "name": "relationship.js",
    "version": "1.2.5",
    "description": "Chinese kinship library",
    "author": "HaoLe Zheng",
    "license": "MIT",
    "keywords": [
        "relationship",
        "relationship.js",
        "chinese",
        "family",
        "kinship",
        "家庭亲戚称谓",
        "亲戚关系算法"
    ],
    "main": "./dist/relationship.min.js",
    "module": "./dist/relationship.min.mjs",
    "exports": {
